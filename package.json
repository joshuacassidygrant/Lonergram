{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "proxy": "http://localhost:5000/",
  "engines": {
    "node": "9.3.0"
  },
  "scripts": {
    "start": "node ./bin/www",
    "build": "npm run build",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "dev": "concurrently \"npm run start\" \"(cd client && npm run start)\""
  },
  "dependencies": {
    "concurrently": "^4.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongodb": "^3.2.7",
    "morgan": "~1.9.1"
  }
}
